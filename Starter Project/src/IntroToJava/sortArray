package IntroToJava;

import java.util.Scanner;

public class sortArray {
	// its just a complicated way of printing everything in an array that is made from user input
	 public static void main(String args[]){       
		 Scanner input = new Scanner(System.in); 
		 System.out.println("enter here");          
		 String word = input.next();   
		 
		 String[] something = new String[word.length()*2];                
		
		 for(int i =0; i<word.length();i++) {
			 something[i]= word.substring(i,i+1);
			 something[i+word.length()]=something[i];
		 }
		 
		 for(int i =0; i<word.length();i++) {
			 System.out.println(something[i]);
		 }
		 
		 
		 
	 }
	
}
